<!doctype html>
<html lang="en" ng-app="businessInsertion">
<head>
	<title>Unscene Business Insertion</title>
	<link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.0.2/css/bootstrap.min.css">
	<link rel="stylesheet" href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.min.css">
	<style>
		body 		{ padding-top:80px; }
	</style>
</head>
<!-- <body data-spy="scroll" data-offset="0" data-target="#nav" ng-controller="CoursesLists"> -->
<body ng-controller="BusinesList">
<div class="container">
<div class="col-sm-6 col-sm-offset-3">

	<h1><span class="fa fa-sign-in"></span> </h1>

	<h1><%= msg %></h1>

	<form action="/" method="post">

		<div class="form-group" ng-repeat="business in businesses">
			<label>{{business.name}}</label>
			<input type="text" class="form-control" name= "{{business.variable}}" >
		</div>
		<button type="submit" class="btn btn-warning btn-lg">Submit</button>
	</form>

	<hr>

</div>
</div>
</body>
<script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.3.15/angular.min.js"></script>
<script src="javascripts/businesses.js"></script>
</html>